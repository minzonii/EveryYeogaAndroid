<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kosta.travel.MainActivity">
    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp">

        <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="369dp" />

    <Button
        android:id="@+id/create_travel_plan_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="create_travel_plan"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="104dp" />

    <Button
        android:id="@+id/my_travel_plan_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="my_travel_plan"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="183dp" />

    <Button
    android:id="@+id/travel_plan_list_btn"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="travel_plan_list"
    tools:layout_editor_absoluteX="114dp"
    tools:layout_editor_absoluteY="267dp" />
</LinearLayout>


</android.support.constraint.ConstraintLayout>
